{
  "name": "peasy-fe",
  "private": true,
  "version": "0.0.43",
  "author": "NightCat<nightcat@hzfe.org>",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "deploy": "sh scripts/deploy.sh",
    "build": "npm run pre-build && vue-tsc --noEmit && npm version patch --no-git-tag-version && vite build",
    "build:analyze": "npm run pre-build && vue-tsc --noEmit && VITE_ANALYZE=true vite build",
    "dev:template": "vue-tsc --noEmit && VITE_IS_TEMPLATE=true vite --config vite.template.config.ts",
    "build:template": "rm -rf template-dist/ && npm run pre-build && vue-tsc --noEmit && VITE_IS_TEMPLATE=true vite build --config vite.template.config.ts && npm run cp:template",
    "cp:template": "cp -r template-dist/* peasy-be/template/",
    "cp:types": "cp -r src/types/config.d.ts peasy-be/types/ && cp -r src/config.ts peasy-be/types/",
    "cp:stylesheet": "cp -r src/components/libs/hooks/stylesheet.ts peasy-be/template/",
    "pre-build": "npm run cp:types && npm run cp:stylesheet",
    "preview": "vite preview",
    "prettier": "npx prettier --write src/"
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@logto/vue": "^1.0.0-beta.2",
    "@sipec/vue3-tags-input": "^3.0.4",
    "@types/sortablejs": "^1.13.0",
    "ahooks-vue": "^0.13.3",
    "ali-oss": "^6.17.1",
    "floating-vue": "^2.0.0-beta.17",
    "html2canvas": "^1.4.1",
    "jsoneditor": "^9.9.0",
    "lodash-es": "^4.17.21",
    "mande": "^2.0.2",
    "mitt": "^3.0.0",
    "moveable": "^0.30.0",
    "panzoom": "^9.4.3",
    "pinia": "^2.0.15",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue-slider-component": "^4.1.0-beta.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.4",
    "@types/jsoneditor": "^9.5.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.0.3",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.5.4",
    "vite": "^3.0.8",
    "vite-plugin-singlefile": "^0.11.1",
    "vite-svg-loader": "^3.4.0",
    "vue-tsc": "^0.34.7"
  }
}
